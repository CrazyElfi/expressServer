{% extends "layout.njk" %}

{% block content %}

<table>
    {% for good in goods %}
        <tr>
            <td class="tableTd">{{ good.id }}</td>
            <td class="tableTd">{{ good.name }}</td>
            <td class="tableTd">{{ good.price }}</td>
{#            <td>{{ good.description }}</td>#}
{#            <td>{{ good.category }}</td>#}
            <td class="tableTd">{{ good.createdAt }}</td>
            <td class="tableTd">{{ good.updatedAt }}</td>
        </tr>
    {% endfor %}
</table>
{% endblock %}